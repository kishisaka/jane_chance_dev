<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="us.ttyl.chance.domain.GoogleMapMarkers" table="t_google_map_markers">
    	   	    
        <id name="markerId" type="integer">
            <column name="marker_id" />
            <generator class="native"></generator>
        </id>        
                                       
        <property name="zipcode" type="string">
        	<column name="zipcode" length="20" />
        </property>
        
        <property name="numberOfUsers" type="integer">
        	<column name="number_of_users" length="20" />
        </property>
   
        <property name="geoInfoLat" type="string">
        	<column name="geo_info_lat" length="255"/>
        </property>  
        
        <property name="geoInfoLong" type="string">
        	<column name="geo_info_long" length="255"/>
        </property>  
        
    </class>
</hibernate-mapping>